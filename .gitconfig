[user]
	name = Oscar Carlsson
	email = ocarlsson3@gmail.com
[core]
    editor = vim
[color]
    branch = auto
    diff = auto
    status = auto
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[pull]
    default = current
[branch]
    autosetuprebase = always
[alias]
    ls = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate
    ll = log --pretty=format:"%C(yellow)%h%Cred%d\\ %Creset%s%Cblue\\ [%cn]" --decorate --numstat
    fl = log -u
    st = status
    cl = clone
    ci = commit
    co = checkout
    br = branch
    diff = diff --word-diff
    dc = diff --cached
